<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  
  <body>
    
    <div id="container" class="js-close-any" canvas="container">
      <div id="wrapper" class="center">
        <div class="row">
          <div class="small-4 medium-3 small-centered columns">
            <img id="home-logo" src="img/human.png"/>
          </div>
        </div>

        <div class="row">
          <h1 class="three-margin-top two-margin-bottom">The League of Gentle Humans</h1>
          <h4>Let's find ways to help each other!</h4>
        </div>
        
        <div class="row three-margin-top">
          <div class="small-12 medium-8 small-centered columns">
            <button class="button large">Enter</button>
          </div>
        </div>
        
        <div id="push"></div>
    
      <!-- wrapper div-->
      </div>
            
    <!--container div -->
    </div>
    
    <div id="bottom-stripe" class="full-width"></div>
    
    <div off-canvas="menu right shift" id="menu">
      <% include ../partials/menu %>
    </div>
  
    <div id="new-popup" class="modal center small-12 medium-9 large-7 small-centered columns">
      <div class="row center">
        <h3 class="popup-title"></h3>
      </div>
      
      <div class="row">
        <div class="small-centered medium-10 columns">
          
          <p></p>
          
          <button class="tax-id-popup_close button">Okay</button>
          <a><button class="button"></button></a>
        </div>
      </div>
    </div>
    
    
    <script type="text/javascript">
      $(document).ready(function(){
        $('#new-popup').popup({
          vertical: 'top',
          transition: 'all 0.75s',
          scrolllock: true,
        });
      });
    </script>
    
    <script src="js/end_scripts.js" type="text/javascript"></script>
  </body>
</html>